docker run --name zabbix-appliance -t       -p 10051:10051       -p 80:80       -d zabbix/zabbix-appliance:latest
docker run --name zabbix-appliance -t       -p 10051:10051       -p 80:80       -d zabbix/zabbix-appliance:iot01
docker run --name zabbix-appliance-iot -t       -p 10051:10051       -p 80:80       -d zabbix/zabbix-appliance:iot01

docker ps - list containers
docker stop - stop container
docker exec -it zabbix-appliance-iot /bin/bash - entrar no container



